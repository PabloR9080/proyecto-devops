pipeline{
    agent {label 'agent1'}
    parameters {
        string(name: 'GIT_BRANCH', defaultValue: 'develop', description: 'Git branch')
        string(name: 'BUILD_NUMBER', defaultValue: 'NoNumber', description: 'Build number')
    }
    stages{
        stage('Test'){
            steps{
                sh 'npm install'
                sh 'npm run test'
            }
        }
        stage('Build'){
            steps{
                sh 'npm run build'
            }
        }
    }
}